      "const fn_api = () => {",
      "\t// ...一系列api操作",
      "\tconst seriesData = [",
      "\t\t{ value: 1048, name: \"Search Engine\" },",
      "\t\t{ value: 735, name: \"Direct\" },",
      "\t\t{ value: 580, name: \"Email\" },",
      "\t\t{ value: 484, name: \"Union Ads\" },",
      "\t\t{ value: 300, name: \"Video Ads\" },",
      "\t];",
      "\tchartInstance.setOption({",
      "\t\tseries: [{ name: \"Access From\", data: seriesData }],",
      "\t});",
      "};",
      "",
      "onUnmounted(() => {",
      "\twindow.removeEventListener(\"resize\", resize);",
      "});",
