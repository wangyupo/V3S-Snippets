  "Echarts pie": {
    "prefix": "v3s-echart-pie",
    "body": [
      "<template>",
      "\t<!-- 业务注释 -->",
      "\t<RhWrapper title=\"饼图\">",
      "\t\t<div class=\"h-[300px]\" ref=\"chartRef\"></div>",
      "\t</RhWrapper>",
      "</template>",
      "",
      "<script setup>",
      "import { onMounted, onUnmounted, ref } from \"vue\";",
