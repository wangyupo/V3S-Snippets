  "create pinia store": {
    "prefix": ["v3s-pinia"],
    "body": [
      "import { defineStore } from \"pinia\";",
      "",
      "export const use${TM_FILENAME_BASE/(.*)/${1:/capitalize}/}Store = defineStore(\"${TM_FILENAME_BASE}\", {",
      "  state: () => ({",
      "    counter: 10,",
      "  }),",
      "  getters: {",
      "    doubleCount: state => state.counter * 2,",
      "  },",
      "  actions: {",
      "    increment(data) {",
      "      this.counter = data;",
      "    },",
      "  },",
      "  persist: true, // localstorage yes or no",
      "});",
      ""
    ]
  },
