{
  "use element-plus message-box": {
    "prefix": "v3s-elmessagebox",
    "body": [
      "${1:import { ElMessage, ElMessageBox \\} from \"element-plus\";}",
      "",
      "const open = () => {",
      "  ElMessageBox.confirm(`弹框内容?`, \"弹框标题\", {",
      "    confirmButtonText: \"确认\",",
      "    cancelButtonText: \"取消\",",
      "    type: \"warning\",",
      "  })",
      "    .then(() => {",
      "      ElMessage({",
      "        type: \"success\",",
      "        message: \"提示语\",",
      "      });",
      "    })",
      "    .catch(() => {});",
      "};",
      ""
    ]
  },
  "create el-message": {
    "prefix": ["v3s-elmsg"],
    "body": [
      "${1:import { ElMessage \\} from \"element-plus\";}",
      "",
      "ElMessage({",
      "  message: \"${2:提示信息}\",",
      "  type: \"${3|success,warning,info,error|}\",",
      "});"
    ],
    "description": "create el-message code"
  }
}
