{
  "table template": {
    "prefix": "v3s-table-template",
    "body": ["<template #$1=\"{ scope }\">", "\t{{ scope.row.$1 }}", "</template>"],
    "description": "v3s table custom column"
  },
  "v3s wrapper": {
    "prefix": "v3s-wrapper",
    "body": ["<RhWrapper title=\"${1:示例标题}\">", "\t$2", "</RhWrapper>"],
    "description": "v3s section wrapper"
  },
  "slot": {
    "prefix": "v3s-slot",
    "body": ["<slot></slot>"]
  },
  "scope row": {
    "prefix": ["v3s-scr"],
    "body": ["scope.row"]
  },
  "element-plus v-loading": {
    "prefix": ["v3s-loading"],
    "body": ["v-loading=\"loading\""]
  },
  "element-plus el-button": {
    "prefix": "v3s-btn",
    "body": ["<el-button type=\"primary\">${1:Primary}</el-button>"]
  },
  "element-plus el-button link": {
    "prefix": "v3s-btnlink",
    "body": ["<el-button type=\"primary\" link>${1:Primary}</el-button>"]
  },
  "element-plus el-input": {
    "prefix": "v3s-input",
    "body": ["<el-input v-model=\"input\" placeholder=\"Please input\" />", "const input = ref('')"]
  }
}
