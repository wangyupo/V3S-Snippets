{
  "table template": {
    "prefix": "v3s-table-template",
    "body": ["<template #$1=\"{ scope }\">", "\t{{ scope.row.$1 }}", "</template>"],
    "description": "v3s table custom column"
  },
  "v3s wrapper": {
    "prefix": "v3s-wrapper",
    "body": ["<RhWrapper title=\"${1:示例标题}\">", "\t$2", "</RhWrapper>"],
    "description": "v3s section wrapper"
  },
  "slot": {
    "prefix": "v3s-slot",
    "body": ["<slot></slot>"]
  },
  "scope row": {
    "prefix": ["v3s-scr"],
    "body": ["scope.row"]
  },
  "element-plus v-loading": {
    "prefix": ["v3s-loading"],
    "body": ["v-loading=\"loading\""]
  },
  "assets": {
    "prefix": ["v3s-assets"],
    "body": ["@/assets/$1"]
  },
  "template": {
    "prefix": ["v3s-template"],
    "body": ["<template>", "\t${1:content}", "</template>"]
  },
  "template with slot": {
    "prefix": ["v3s-template-slot"],
    "body": ["<template #${1:slot-name}>", "\t${2:content}", "</template>"]
  },
  "v-for": {
    "prefix": ["v3s-v-for"],
    "body": ["v-for=\"(${1:item}, ${2:index}) in ${3:array}\" :key=\"${2:index}\""]
  }
}
